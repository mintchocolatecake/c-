int fibonacci(int num) {
	if (1 == num || 2 == num) {
		return 1;
	}
	int value = fibonacci(num - 1) + fibonacci(num - 2);
	return value;
}
int main() {
	/*int value = Factorial_Re(5);
	std::cout << value;
	return value;*/
	int a = fibonacci(9);
	std::cout << a;
}
